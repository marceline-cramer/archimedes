{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "fulcrum",
  "rules": {
    "file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "rule"
          },
          {
            "type": "SYMBOL",
            "name": "decision"
          },
          {
            "type": "SYMBOL",
            "name": "constraint"
          }
        ]
      }
    },
    "_whitespace": {
      "type": "PATTERN",
      "value": "[ \\n\\r\\t]"
    },
    "comment": {
      "type": "PATTERN",
      "value": ";.*\\n"
    },
    "variable": {
      "type": "PATTERN",
      "value": "[a-z][a-z0-9_]*"
    },
    "symbol": {
      "type": "PATTERN",
      "value": "[A-Z][a-zA-Z0-9]*"
    },
    "_ident": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "SYMBOL",
          "name": "symbol"
        }
      ]
    },
    "integer": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "0"
        },
        {
          "type": "PATTERN",
          "value": "-?[1-9][0-9]*"
        }
      ]
    },
    "decision": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "decide"
        },
        {
          "type": "SYMBOL",
          "name": "rule"
        }
      ]
    },
    "rule": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "head",
          "content": {
            "type": "SYMBOL",
            "name": "atom"
          }
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "if"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_rule_body"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "."
        }
      ]
    },
    "_rule_body": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "atom"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "atom"
              }
            ]
          }
        }
      ]
    },
    "constraint": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "constrain"
        },
        {
          "type": "FIELD",
          "name": "captures",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "captures"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "FIELD",
          "name": "kind",
          "content": {
            "type": "SYMBOL",
            "name": "constraint_kind"
          }
        },
        {
          "type": "FIELD",
          "name": "bound",
          "content": {
            "type": "SYMBOL",
            "name": "integer"
          }
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "SYMBOL",
            "name": "_rule_body"
          }
        },
        {
          "type": "STRING",
          "value": "."
        }
      ]
    },
    "captures": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_ident"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_ident"
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "constraint_kind": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "cardinality"
        }
      ]
    },
    "cardinality": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "cardinality"
        },
        {
          "type": "STRING",
          "value": "to"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "only"
            },
            {
              "type": "SYMBOL",
              "name": "at_most"
            },
            {
              "type": "SYMBOL",
              "name": "at_least"
            }
          ]
        }
      ]
    },
    "only": {
      "type": "STRING",
      "value": "only"
    },
    "at_most": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "at"
        },
        {
          "type": "STRING",
          "value": "most"
        }
      ]
    },
    "at_least": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "at"
        },
        {
          "type": "STRING",
          "value": "least"
        }
      ]
    },
    "atom": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "relation",
          "content": {
            "type": "SYMBOL",
            "name": "symbol"
          }
        },
        {
          "type": "SYMBOL",
          "name": "pattern"
        }
      ]
    },
    "pattern": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "tuple"
        },
        {
          "type": "SYMBOL",
          "name": "_ident"
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        }
      ]
    },
    "tuple": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "pattern"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "SYMBOL",
                    "name": "pattern"
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "record": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "field"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "SYMBOL",
                    "name": "field"
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "field": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "symbol"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "pattern"
          }
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "_whitespace"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
